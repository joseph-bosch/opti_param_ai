# MSSQL_DRIVER=ODBC Driver 17 for SQL Server
MSSQL_DRIVER=ODBC Driver 18 for SQL Server

# Training data
TRAIN_CSV=C:\Users\lenovo\Desktop\Dev\Opti_param\opti_param_ai\training\Data_1.xlsx
# TRAIN_CSV=C:\Users\OLJ1SGH\Desktop\Dev\Opti_param\opti_param_ai\training\Synthetic_Data.xlsx
# TRIALS_CSV=./models/optuna_trials_v1.2.62.csv

# Model storage
MODEL_DIR=..\models
MODEL_VERSION=v2.4
# MODEL_VERSION=cb_v0.1


OPTUNA_STUDY_NAME=opti_v1.65
OPTUNA_STORAGE_TYPE=mssql
SAVE_TRIALS_TO_DB=1        # 0 or 1
TRIALS_TABLE=optuna_trials
OPTUNA_PARALLEL_JOBS=1

MULTI_ALGO_MODE=roundrobin
ALGO_ORDER=nn,lgb,cat,xgb
# ALGO_ORDER=xgb,cat,lgb,nn
ROUND_TRIALS=15000
# (optional per-algo overrides)
TARGET_TRIALS_NN=15000
TARGET_TRIALS_XGB=30000
TARGET_TRIALS_CAT=30000
TARGET_TRIALS_LGB=30000

ALGO_PROCESSES=4
NN_PROGRESS=0



# Cross-validation
USE_CV=1
CV_METHOD=timeseries
N_SPLITS=4
GAP=3
EARLY_STOP_ROUNDS=120

# Robust labels + objective
TARGET_CLIP_Q=0.02,0.98
XGB_OBJECTIVE=reg:squarederror   # will auto-fallback if unsupported

# Feature pruning / selection
DROP_CONSTANTS=1
CORR_DROP_THR=0.995
SELECT_TOP_NUMERIC=999
ALWAYS_KEEP_PATTERNS=Axis-X_Upper_Point,Axis-Z_Upper_Point,Axis-Z_Lower_Point,Gun1_Voltage,Gun2_Voltage,Gun1_Powder,Gun2_Powder,Time_Index

# Engineered features
USE_FEAT_ENG=1
USE_TIME_INDEX=1
TIME_INDEX_SCALE=1.0
USE_EXPOSURES=0                # start with 0; set to 1 after fix
EXPO_SPEED_FLOOR_Q=0.20        # floor speed at 20th percentile per station
EXPO_CLIP_QHI=0.995            # clip exposure at 99.5th percentile
EXPO_USE_LOG=1                 # use log1p(exposure)
EXPOSURE_EPS=0.001

# Domain priors / monotonic constraints
USE_MONO=0
MONO_POS=Voltage,Powder,total_powder,total_voltage,exposure
MONO_NEG=Speed

# Training & acceptance
TUNE=0
ACCEPT_WNRMSE=0.30
RANDOM_STATE=42

# --- New flags for CatBoost integration ---
USE_CATBOOST=1                 # 1 = train CatBoost multi-output alongside XGBoost, 0 = skip
CATBOOST_LOSS=MultiRMSE        # multi-target loss
CATBOOST_EARLY_STOP=100        # early stopping rounds for CatBoost
CATBOOST_DEPTH=8               # default tree depth (tune later if needed)
CATBOOST_LR=0.80               # learning rate
